  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.3.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.3.0",
    "ag-grid-community": "21.0.1",
    "ag-grid-react": "21.0.1",
    "apexcharts": "^3.8.3",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "brototype": "0.0.6",
    "classnames": "^2.2.6",
    "date-fns": "^2.0.0-beta.5",
    "font-awesome": "^4.7.0",
    "husky": "^3.1.0",
    "js-file-download": "^0.4.9",
    "kg-grid": "https://github.com/kgujral/kg-grid/tarball/master",
    "lint-staged": "^9.5.0",
    "mui-datatables": "^2.6.4",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "react": "^16.8.6",
    "react-apexcharts": "^1.3.3",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.8.6",
    "react-dropzone": "^11.0.2",
    "react-google-maps": "^9.4.5",
    "react-hook-form": "^6.0.8",
    "react-icons": "^3.10.0",
    "react-promise-tracker": "^2.0.5",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.1.0",
    "react-switch": "^5.0.1",
    "react-syntax-highlighter": "^11.0.2",
    "react-toastify": "^6.0.0-rc.3",
    "reactstrap": "^8.5.1",
    "recharts": "^1.6.2",
    "tinycolor2": "^1.4.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-app-env --env-file=config/local.env start",
    "startdev": "react-app-env --env-file=config/dev.env start",
    "local": "react-app-env --env-file=config/local.env build",
    "dev": "react-app-env --env-file=config/dev.env build",
    "qa": "react-app-env --env-file=config/qa.env build",
    "stg": "react-app-env --env-file=config/stg.env build",
    "uat": "react-app-env --env-file=config/uat.env build",
    "prod": "react-app-env --env-file=config/prod.env build",
    "test": "react-app-env test",
    "eject": "react-scripts eject",
    "format-project": "./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "react-app-env": "^1.2.3"
  }
}
